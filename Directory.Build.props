<Project>
  <PropertyGroup>
    <Company>NordAPI</Company>
    <Authors>NordAPI</Authors>

    <RepositoryUrl>https://github.com/NordAPI/NordAPI.SwishSdk</RepositoryUrl>
    <PackageProjectUrl>https://github.com/NordAPI/NordAPI.SwishSdk</PackageProjectUrl>
    <RepositoryType>git</RepositoryType>

    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageTags>swish;payments;sdk;nordapi</PackageTags>

    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>

    <!-- README visas på NuGet; själva filen packas via .csproj -->
    <PackageReadmeFile>README.md</PackageReadmeFile>

    <!-- Bygg-kvalitet -->
    <Deterministic>true</Deterministic>
    <ContinuousIntegrationBuild>true</ContinuousIntegrationBuild>
    <TreatWarningsAsErrors>false</TreatWarningsAsErrors>

    <!-- VERSION FÖR DENNA RELEASE -->
    <Version>1.0.2</Version>
  </PropertyGroup>
</Project>
